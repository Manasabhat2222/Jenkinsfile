pipeline {
    agent {label 'master'}
    parameters {
    string description: 'Enter the Tomcat version link to be downloaded', name: 'TomcatLink', trim: true
    }
    environment {
      link = "$TomcatLink"
    }

    stages {
        stage ('Install Java') {
            steps {
                sh '''
                wget "${link}"
                '''
                }
            }
        
        }
    }
